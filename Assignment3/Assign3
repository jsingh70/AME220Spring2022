rgb(213, 12,112)
	Hex: #D50670
	213 = 16 * 13 + 1 * 5 = D5
	12 = 16 * 0 + 1 * 12 = 06
	112 = 16 * 7 + 1 * 0 = 70
	
	
rgb(22,126,52)
	Hex: #167E34
	22 = 16 * 1 + 1 * 6 = 16
	126 = 7 * 0 + 1 * 14 = 7E
	52 = 16 * 3 + 1 * 4 = 34
	
	
rgb(213,214,41)
	Hex: #D5D629
	213 = 16 * 13 + 1 * 5 = D5
	213 = 16 * 13 + 1 * 6 = D6
	52 = 16 * 2 + 1 * 9 = 29
	
	
rgb(113,22,113)
	Hex: #711671
	113 = 16 * 7 + 1 * 1 = 71
	22 = 16 * 1 + 1 * 6 = 16
	113 = 16 * 7 + 1 * 1 = 71
	
	
rgb(126,112,172)
	Hex: #7E70AB
	126 = 16 * 7 + 1 * 14 = 7E
	112 = 16 * 7 + 1 * 0 = 70
	172 = 16 * 10 + 1 * 11 = AB
	
	
rgb(142,24,241)
	Hex: #8E18F1
	142 = 16 * 8 + 1 * 14 = 8E
	24 = 16 * 1 + 1 * 8 = 18
	241 = 16 * 15 + 1 * 1 = F1
	
	
rgb(123,32,121)
	Hex: #7B2079
	123 = 16 * 7 + 1 * 11 = 7B
	32 = 16 * 2 + 1 * 0 = 20
	121 = 16 * 7 + 1 * 9 = 79
	
	
rgba(127,123,224,.4)
	Hex: #7F7BE0
	127 = 16 * 7 + 1 * 15 = 7F
	123 = 16 * 7 + 1 * 11 = 7B
	224 = 16 * 14 + 1 * 0 = E0
	.4 = 66
	
	
	
	
